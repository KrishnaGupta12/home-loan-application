package com.lti.HomeLoanApplication.Entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="loaneligibility")
public class Eligibility {
	
	private Integer id;
	private Integer age;
	private String employement_type;
	private Integer income;
	private String ctc;
	
	@Id
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	
	@Column(name = "age", nullable = false)
	public Integer getAge() {
		return age;
	}
	public void setAge(Integer age) {
		this.age = age;
	}
	@Column(name = "employement_type", nullable = false)
	public String getEmployement_type() {
		return employement_type;
	}
	public void setEmployement_type(String employement_type) {
		this.employement_type = employement_type;
	}
	@Column(name = "income", nullable = false)
	public Integer getIncome() {
		return income;
	}
	public void setIncome(Integer income) {
		this.income = income;
	}
	@Column(name = "ctc", nullable = false)
	public String getCtc() {
		return ctc;
	}
	public void setCtc(String ctc) {
		this.ctc = ctc;
	}
	@Override
	public String toString() {
		return "Eligibility [id=" + id + ", age=" + age + ", employement_type=" + employement_type + ", income="
				+ income + ", ctc=" + ctc + "]";
	}
	
	

}
 